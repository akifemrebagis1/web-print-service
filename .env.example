# Web Print Service - Environment Variables
# Bu dosyayı .env olarak kopyalayın ve kendi değerlerinizi girin

# Flask Ayarları
FLASK_ENV=development
FLASK_DEBUG=True
SECRET_KEY=your-super-secret-key-change-this-in-production

# Sunucu Ayarları
FLASK_HOST=0.0.0.0
FLASK_PORT=5000

# Upload Ayarları (opsiyonel - varsayılan: proje dizini/uploads)
# UPLOAD_FOLDER=/path/to/your/uploads
# TEMPLATES_FOLDER=/path/to/your/templates

# Maksimum dosya boyutu (byte cinsinden, varsayılan: 100MB)
# MAX_CONTENT_LENGTH=104857600

# Tarayıcı otomatik açma (development için True)
AUTO_OPEN_BROWSER=True
